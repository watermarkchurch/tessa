{
  "extends": [
    "config:base"
  ],
  "rangeStrategy": "replace",
  "dependencyDashboard": true,
  "major": {
    "dependencyDashboardApproval": true
  },
  "enabledManagers": [
    "bundler",
    "circleci",
    "dockerfile",
    "ruby-version"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "excludePackagePatterns": ["^ruby"],
      "enabled": false
    }
  ]
}
